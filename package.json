{
  "name": "booking-webpage",
  "version": "1.0.0",
  "description": "From the Udemy online course (Udemy - Advanced CSS and Sass Flexbox, Grid, Animations and More!) by Jonas Schmedtmann. This is thesecond project, I call it Booking project, but by the lecturer it is called Trillo project. You can go online to udemy to check this amzing project or follow the link to view the complete project of my work.",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.compile.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.compile.css",
    "prefix:css": "postcss css/style.compile.css --use autoprefixer -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AssouaAlbert/Booking-Webpage.git"
  },
  "keywords": [
    "Eloumbat",
    "Assoua",
    "Albert",
    "Udemy",
    "Jonas",
    "Schmedtmann",
    "Udemy",
    "CSS",
    "Flexbox",
    "Project",
    "Booking",
    "Trillo"
  ],
  "author": "Eloumbat Assoua Albert and Jonas Schmedtmann",
  "license": "ISC",
  "browserslist": ["last 19 version"],
  "bugs": {
    "url": "https://github.com/AssouaAlbert/Booking-Webpage/issues"
  },
  "homepage": "https://github.com/AssouaAlbert/Booking-Webpage#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "concat": "^1.0.3",
    "node-sass": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
}
